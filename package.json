{
  "name": "caiji-games",
  "private": true,
  "packageManager": "yarn@4.9.2",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --all -pt run build",
    "build:web": "yarn workspaces foreach --all -pt run build:web",
    "build:desktop": "yarn workspaces foreach --all -pt run build:desktop",
    "tauri": "yarn workspaces foreach --all run tauri",
    "tauri:ms": "yarn workspace minesweeper tauri:ms"
  }
}
